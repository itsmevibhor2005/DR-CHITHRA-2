{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1",
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate"
        }
      },
      {
        "src": "/.*",
        "dest": "/dist/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }